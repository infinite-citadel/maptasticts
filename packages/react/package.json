{
  "name": "maptastic-react",
  "version": "0.0.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "pkgroll --sourcemap",
    "watch:build": "pkgroll --watch --sourcemap"
  },
  "devDependencies": {
    "pkgroll": "^2.17.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@types/react": "^19.0.0",
    "maptastic": "workspace:*",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
